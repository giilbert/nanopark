[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://nanopark-glue/target/debug/libglue.so"
linux.release.x86_64 =   "res://nanopark-glue/target/release/libglue.so"
windows.debug.x86_64 =   "res://nanopark-glue/target/debug/libglue.dll"
windows.release.x86_64 = "res://nanopark-glue/target/release/libglue.dll"
macos.debug =            "res://nanopark-glue/target/debug/libglue.dylib"
macos.release =          "res://nanopark-glue/target/release/libglue.dylib"
macos.debug.arm64 =      "res://nanopark-glue/target/debug/libglue.dylib"
macos.release.arm64 =    "res://nanopark-glue/target/release/libglue.dylib"
